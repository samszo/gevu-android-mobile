<?xml version="1.0" encoding="utf-8"?>
<s:View xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" title="gevu_geos">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	<fx:Script>
		<![CDATA[
			import flashx.textLayout.formats.Float;
			[Bindable] public var vis:Boolean;
			[Bindable] public var adresse:String;
			[Bindable] public var lat:Number;
			[Bindable] public var lng:Number;
			
			
		]]>
	</fx:Script>
	
	<s:VGroup>
		
		<s:HGroup width="100%">
			<s:Label text="adresse" />
			<s:Scroller>
			<s:Group>	
			  <s:TextArea id="F01"  text="{this.adresse}" width="240"/>
			</s:Group>	
			</s:Scroller>	
		</s:HGroup>
		<s:HGroup width="100%">
			<s:Label text="lat." width="55"/>
			<s:TextInput id="F02"  text="{this.lat}" width="175"/>	
		</s:HGroup>	
		<s:HGroup>
			<s:Label text="lng." width="55"/>
			<s:TextInput id="F03" text="{this.lng}" width="175"/>
		</s:HGroup>
	</s:VGroup>
</s:View>
